<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
 <class name="Model.Film" table="FILM">
  <id name="idFilm" type="int">
   <column name="IDFILM"/>
   <generator class="assigned"/>
  </id>
  <property generated="never" lazy="false" name="titre" type="java.lang.String">
   <column name="TITRE" sql-type="VARCHAR2"/>
  </property>
  <property generated="never" lazy="false" name="anneeSortie" type="int">
   <column name="ANNEESORTIE"/>
  </property>
  <property generated="never" lazy="false" name="langueOriginale" type="java.lang.String">
   <column name="LANGUEORIGINALE"/>
  </property>
  <property generated="never" lazy="false" name="duree" type="int">
   <column name="DUREEMINUTE"/>
  </property>
  <property generated="never" lazy="false" name="resume" type="java.lang.String">
   <column name="RESUME"/>
  </property>
  <property generated="never" lazy="false" name="photo" type="java.lang.String">
   <column name="LIENPOSTER"/>
  </property>
 </class>
</hibernate-mapping>
